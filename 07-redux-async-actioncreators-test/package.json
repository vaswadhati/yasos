{
  "name": "testing-async-action-creators",
  "version": "0.1.0",
  "description": "Testing redux async action creators",
  "author": "Hemalatha Manickam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:vaswadhati/yasos.git"
  },
  "keywords": [
    "ES6",
    "redux",
    "jest"
  ],
  "scripts": {
    "lint": "eslint src --cache",
    "build": "webpack --colors --progress -p --env.production --config src/cfg/webpack",
    "watch": "webpack-dev-server --progress --config src/cfg/webpack",
    "start": "npm run build && live-server build/",
    "test": "jest"
  },
  "jest": {
    "moduleDirectories": [
      "src/app/state"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/app/state/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions",
      "last 2 safari versions"
    ],
    "production": [
      "last 3 chrome versions",
      "last 3 firefox versions",
      "last 3 edge versions",
      "last 3 safari versions"
    ]
  }
}